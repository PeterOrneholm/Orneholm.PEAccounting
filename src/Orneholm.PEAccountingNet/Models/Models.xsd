										<xs:complexType>
											<xs:sequence>
												<xs:element name="dimension-entry" type="dimension-entry-reference"
															minOccurs="0" maxOccurs="unbounded"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>

									<!-- Period used for accrual, may only be used for result accounts -->
									<xs:element name="period" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="start-date" type="xs:date"/>
												<xs:element name="end-date" type="xs:date"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>

								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="client-invoice-relations" minOccurs="0">
